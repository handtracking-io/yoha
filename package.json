    "ts-loader": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-backend-webgl": "^3.12.0",
